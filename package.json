  "devDependencies": {
    "@angular-devkit/core": "^15.2.10",
    "@angular/cli": "^15.2.10",
    "@angular/common": "^15.2.10",
    "@angular/compiler": "^15.2.10",
    "@angular/compiler-cli": "^15.2.10",
    "@angular/core": "^15.2.10",
    "@angular/platform-browser": "^15.2.10",
    "ng-packagr": "^15.2.0",
    "rimraf": "^3.0.0",
    "rxjs": "~7.5.7",
    "tslib": "^2.3.0",
    "tslint": "^6.1.3",
    "typescript": "^4.9.3",
    "zone.js": "~0.11.8"
  }
