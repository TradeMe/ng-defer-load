{
  "$schema": "./node_modules/ng-packagr/package.schema.json",
  "name": "@trademe/ng-defer-load",
  "version": "8.2.2",
  "description": "Angular directive to load elements lazily",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "scripts": {
    "clean": "rimraf ./compiled && rimraf ./dist",
    "prebuild": "npm run clean",
    "lint": "tslint -c tslint.json 'src/**/*.ts'",
    "build": "ng-packagr -p package.json",
    "prepublishOnly": "npm run lint && npm run build"
  },
  "ngPackage": {
    "lib": {
      "entryFile": "src/public_api.ts"
    }
  },
  "author": "Vamsi Vempati, Michael Zoellner, Nick Webster",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/TradeMe/ng-defer-load"
  },
  "keywords": [
    "angular",
    "lazyload",
    "defer",
    "deferload",
    "intersection",
    "intersectionobserver"
  ],
  "dependencies": {
    "tslib": "^1.9.0"
  },
  "peerDependencies": {
    "@angular/common": ">=8.0.0",
    "@angular/core": ">=8.0.0",
    "@angular/platform-browser": ">=8.0.0",
    "rxjs": "^6.4.0",
    "zone.js": ">=0.9.1"
  },
  "devDependencies": {
    "@angular/common": "8.2.14",
    "@angular/compiler": "8.2.14",
    "@angular/compiler-cli": "8.2.14",
    "@angular/core": "11.0.5",
    "@angular/platform-browser": "8.2.14",
    "ng-packagr": "5.7.1",
    "rimraf": "^3.0.0",
    "rxjs": "^6.4.0",
    "tslint": "^5.20.0",
    "typescript": "~3.5.3",
    "zone.js": "~0.9.1"
  }
}
