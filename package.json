{
  "name": "@trademe/ng-defer-load",
  "version": "2.0.0",
  "description": "Angular directive to load elements lazily",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "scripts": {
    "clean": "rimraf ./compiled && rimraf ./dist",
    "prebuild": "npm run clean",
    "lint": "tslint -c tslint.json 'src/**/*.ts'",
    "build": "ngc -p ./tsconfig.json && tsc -p ./tsconfig.json",
    "prepublishOnly": "npm run lint && npm run build"
  },
  "author": "Vamsi Vempati",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/TradeMe/ng-defer-load"
  },
  "keywords": [
    "angular",
    "lazyload",
    "defer",
    "deferload",
    "intersection",
    "intersectionobserver"
  ],
  "peerDependencies": {
    "@angular/common": "^6.0.0",
    "@angular/core": "^6.0.0",
    "@angular/platform-browser": "^6.0.0",
    "rxjs": "^6.0.0",
    "tslib": "^1.8.1",
    "zone.js": "^0.8.20"
  },
  "devDependencies": {
    "@angular/common": "^6.0.0",
    "@angular/compiler": "^6.0.0",
    "@angular/compiler-cli": "^6.0.0",
    "@angular/core": "^6.0.0",
    "@angular/platform-browser": "^6.0.0",
    "rimraf": "^2.6.2",
    "rxjs": "^6.2.1",
    "tslib": "^1.8.1",
    "tslint": "^5.9.1",
    "typescript": "^2.6.2",
    "zone.js": "^0.8.20"
  }
}
